{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "entrypoints/**/*.{ts,tsx}",
    "src/**/*.{ts,tsx}",
    "utils/**/*.{ts,tsx}",
    "e2e/**/*.{ts,tsx}",
    "wxt.config.ts",
    "vitest.config.ts",
    "eslint.config.js",
    "playwright.config.ts"
  ],
  "project": [
    "src/**/*.{ts,tsx}",
    "entrypoints/**/*.{ts,tsx}",
    "utils/**/*.{ts,tsx}",
    "e2e/**/*.{ts,tsx}"
  ],
  "ignore": [
    ".output/**",
    ".wxt/**",
    "test-results/**",
    "node_modules/**",
    "dist/**",
    "build/**",
    "vitest.config.ts",
    "eslint.config.js",
    "playwright.config.ts"
  ],
  "ignoreDependencies": [
    "@wxt-dev/module-react"
  ],
  "ignoreExportsUsedInFile": true,
  "includeEntryExports": true,
  "workspaces": {
    ".": {
      "entry": [
        "entrypoints/**/*.{ts,tsx}",
        "src/**/*.{ts,tsx}",
        "utils/**/*.{ts,tsx}",
        "e2e/**/*.{ts,tsx}",
        "wxt.config.ts",
        "vitest.config.ts",
        "eslint.config.js",
        "playwright.config.ts"
      ]
    }
  }
}
