{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "ignore": [
    ".output/**",
    ".wxt/**",
    "test-results/**",
    "node_modules/**",
    "dist/**",
    "build/**",
    "vitest.config.ts",
    "eslint.config.js",
    "playwright.config.ts",
    "entrypoints/background.ts",
    "entrypoints/content.ts",
    "wxt.config.ts"
  ],
  "ignoreDependencies": [
    "@wxt-dev/module-react"
  ],
  "ignoreExportsUsedInFile": false,
  "includeEntryExports": true,
  "entry": [
    "entrypoints/**/*.{ts,tsx}!",
    "wxt.config.ts"
  ],
  "project": [
    "src/**/*.{ts,tsx}!",
    "entrypoints/**/*.{ts,tsx}!",
    "wxt.config.ts"
  ]
}
